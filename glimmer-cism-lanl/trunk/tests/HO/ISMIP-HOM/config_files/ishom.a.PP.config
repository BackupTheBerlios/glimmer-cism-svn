[ISMIP-HOM-A]
temperature = -30. 1.67e-5

[parameters]
geothermal = -42e-3
flow_factor = 1

[CF output]
variables = uvelhom vvelhom wvel tau_xz tau_yz
frequency = 100
name = ishom.a.PP.out.nc

[grid]
upn = 21
ewn = 41
nsn = 41
dew = 3200   
dns = 3200   
sigma_builtin = 1

[time]
niso = 1.
tend = 200010.
ntem = 1.
tstart = 200000.
dt = 1.
nvel = 1.

[CF default]
comment = generated by runGlimmer.py
title = ISMIP-HOM Experiment A

[options]
flow_law = 2
evolution = 0
temperature = 1
vertical_integration = 1
sliding_law = 4
marine_margin = 2
stress_calc = 2
hotstart = 1
basal_water = 0
isostasy = 0
periodic_ew = 1
periodic_ns = 1
diagnostic_run = 1

[ho_options]
diagnostic_scheme = 3
prognostic_scheme = 0
which_ho_sparse = 1
which_ho_babc = 4
which_ho_resid = 1
which_ho_efvs = 0

[CF input]
name = ishom.a.nc
time = 1
