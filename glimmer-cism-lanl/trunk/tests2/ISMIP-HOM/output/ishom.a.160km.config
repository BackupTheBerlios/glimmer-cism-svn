[ISMIP-HOM-A]
temperature = -30. 1.67e-5

[parameters]
geothermal = -42e-3
flow_factor = 1

[CF output]
variables = thk btemp temp uvel vvel wvel uvelhom vvelhom wgrd acab diffu uvelhom_srf uvelhom_bas vvelhom_srf vvelhom_bas tau_xz tau_yz
frequency = 1
name = output/ishom.a.160km.out.nc

[ho_options]
prognostic_scheme = 0
diagnostic_scheme = 2
which_ho_sparse = 1

[grid]
upn = 18
ewn = 26
nsn = 26
dew = 6400
dns = 6400
sigma_builtin = 1

[time]
niso = 1.
tend = 200010.
ntem = 1.
tstart = 200000.
dt = 1.
nvel = 1.

[CF default]
comment = generated by runGlimmer.py
title = ISMIP-HOM Experiment A

[options]
vertical_integration = 1
evolution = 0
temperature = 1
flow_law = 2
sliding_law = 4
marine_margin = 2
stress_calc = 2
periodic_ew = 1
hotstart = 1
periodic_ns = 1
diagnostic_run = 1
basal_water = 0
isostasy = 0

[CF input]
name = output/ishom.a.160km.nc
time = 1

