[ISMIP-HOM-A]
temperature = -30. 1.67e-5

[parameters]
geothermal = -42e-3
flow_factor = 1
ice_limit = 0
[CF output]
variables = thk temp uvelhom vvelhom velnormhom tau_xz tau_yz
frequency = 1
name = bproc.out.nc

[grid]
upn = 11
ewn = 45
nsn = 22
dew = 5000
dns = 5000
sigma_builtin = 1

[time]
niso = 1.
tend = 200005.
ntem = 1.
tstart = 200000.
dt = 1.
nvel = 1.

[CF default]
comment = none
title = Coupled ice sheet / Basal Processes Model 

[options]
flow_law = 2
evolution = 3
temperature = 1
vertical_integration = 1
sliding_law = 4
marine_margin = 2
stress_calc = 2
hotstart = 1
basal_water = 3
isostasy = 0
diagnostic_run = 0
which_bmod = 2

[ho_options]
diagnostic_scheme = 3
prognostic_scheme = 0
which_ho_babc = 9
which_ho_resid = 1
which_ho_sparse = 1

[CF input]
name = bproc.nc
time = 1
