[ISMIP-HOM-TEST]

[parameters]
flow_factor = 1

[CF output]
variables = uvelhom vvelhom wvel tau_xz tau_yz
frequency = 1
name = ishom.a.out.nc

[grid]
upn = 21
ewn = 41
nsn = 41
dew = 3200   
dns = 3200   
sigma_builtin = 1

[time]
tstart = 0.
tend = 1.
dt = 1.

[CF default]
comment = generated by runGlimmer.py
title = ISMIP-HOM Experiment A

[options]
flow_law = 2
evolution = 0
periodic_ew = 1
periodic_ns = 1
diagnostic_run = 1

[ho_options]
diagnostic_scheme = 3
which_ho_sparse = 1
which_ho_babc = 4
which_ho_resid = 1
which_ho_efvs = 0

[CF input]
name = ishom.a.nc
time = 1
