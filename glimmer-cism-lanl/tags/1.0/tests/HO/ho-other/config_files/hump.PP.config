[ISMIP-HOM-A]
temperature = -30. 1.67e-5

[parameters]
geothermal = -42e-3
flow_factor = 1
ice_limit = 0

[CF output]
variables = thk temp wvel uvelhom vvelhom 
frequency = 100
name = hump.PP.out.nc

[grid]
upn = 11
ewn = 30
nsn = 30
dew = 2000
dns = 2000
sigma_builtin = 1

[time]
niso = 1.
tend = 201000.
ntem = 1.
tstart = 200000.
dt = 1.
nvel = 1.

[CF default]
comment = written by hump.py
title = Hump Experiment

[options]
flow_law = 2
evolution = 3
temperature = 1
vertical_integration = 1
sliding_law = 4
marine_margin = 2
stress_calc = 2
hotstart = 1
basal_water = 0
isostasy = 0
diagnostic_run = 1

[ho_options]
diagnostic_scheme = 3
prognostic_scheme = 0
which_ho_babc = 4
which_ho_efvs = 0
which_ho_resid = 0
which_ho_sparse = 1

[CF input]
name = ../../tests/HO/ho-other/config_files/hump.nc
time = 1
